@model List<GetTagCloudByBlogIdDto>
<div class="tag-widget post-tag-container mb-5 mt-5">
    <div class="tagcloud">
        @if (Model.Count > 0)
        {

            foreach (var item in Model)
            {
                <a href="#" class="tag-cloud-link">@item.Title</a>
            }

        }
    </div>
</div>