@model List<ResultFooterAddressDto>
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            $('#sartlarVeKosullar').click(function () {

                swal("Şartlar & Koşullar", "Araba kiralama sürecine dair tüm hizmet koşlullarına rezervasyonun fiziki olarak gerçekleştirebileceği ofislerimiz yer alan anlaşma dökümanlarına ulaşabilirsiniz. Bütün sürecin hasar, ceze, geçişler(hgs, ogs vs.) detaylar yer almaktadır. Keyifli yolculuklar dileriz");
            });
            $('#guvenlik').click(function () {

                swal("Güvenlik", "CarBook web sitemiz AspNet Core teknolojisinde Sha256 şifreleme tekniğiyle verilerin tamammen şifre olarak tutulması altyapısına sahiptir sistemimizde Jwt doğrulaması yapılamamktadır. Kullanıcılarımızn ödeme yaptıkları kart bilgileri veritabanımızda kesinlikle sağlanmamaktadır. TC. kimlik numarası, Mail Adresi ve Telefon bilgileri şifrelenmiş halde tutulmaktadır.");
            });
            $('#cerezler').click(function () {

                swal("Çerezler", "Sizlere daha iyi hizmet verebilmek adına tarayıcı çerezlerini kullanmaktayız.");
            });
            $('#sikcaSorulanSorular').click(function () {

                swal("Sıkça Sorulan Sorular", "Sistem ile ilgili sık sorulan sorular yer alır.");
            });
            $('#odemeYontemleri').click(function () {

                swal("Ödeme Yöntemleri", "Ödeme işlemleri yanlızca fiziki lokasyonlarımızdaki kiralama noktalarında kart ile yapılmaktadır.");
            });
            $('#nasilCalisir').click(function () {

                swal("Nasıl Çalışır", "1.Adım: Aracınızı kiralayın. \n 2.Adım: Mailinizi kontrol edin. \n 3.Adım: Ofisimize Gelin");
            });
        });

    </script>


    @foreach (var item in Model)
    {
        <footer class="ftco-footer ftco-bg-dark ftco-section">
            <div class="container">
                <div class="row mb-5">
                    <div class="col-md">
                        <div class="ftco-footer-widget mb-4">
                            <h2 class="ftco-heading-2"><a href="#" class="logo">Car<span>book</span></a></h2>
                            <p>@item.Description</p>
                            <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-5">
                                <li class="ftco-animate"><a href="#"><span class="icon-twitter"></span></a></li>
                                <li class="ftco-animate"><a href="#"><span class="icon-facebook"></span></a></li>
                                <li class="ftco-animate"><a href="#"><span class="icon-instagram"></span></a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md">
                        <div class="ftco-footer-widget mb-4 ml-md-5">
                            <h2 class="ftco-heading-2">Hızlı Linkler</h2>
                            <ul class="list-unstyled">
                                <li><a asp-action="Index" asp-controller="About" class="py-2 d-block">Hakkımızda</a></li>
                                <li><a asp-action="Index" asp-controller="Service" class="py-2 d-block">Hizmetler</a></li>
                                <li><a href="#" id="sartlarVeKosullar" onclick="sweet()" class="py-2 d-block">Şartlar & Koşullar</a></li>
                                <li><a href="#" id="guvenlik" class="py-2 d-block">Güvenlik</a></li>
                                <li><a href="#" id="cerezler" class="py-2 d-block">Çerez Politikası</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md">
                        <div class="ftco-footer-widget mb-4">
                            <h2 class="ftco-heading-2">Müşteri Desteği</h2>
                            <ul class="list-unstyled">
                                <li><a href="#" id="sikcaSorulanSorular" class="py-2 d-block">Sıkça Sorulanlar</a></li>
                                <li><a href="#" id="odemeYontemleri" class="py-2 d-block">Ödeme Yöntemleri</a></li>
                                <li><a asp-action="Index" asp-controller="CarPricing" class="py-2 d-block">Kiralama</a></li>
                                <li><a href="#" id="nasilCalisir" class="py-2 d-block">Nasıl Çalışır</a></li>
                                <li><a asp-action="Index" asp-controller="Contact" class="py-2 d-block">Bize Yazın</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md">
                        <div class="ftco-footer-widget mb-4">
                            <h2 class="ftco-heading-2">Bir Sorunuz mu var?</h2>
                            <div class="block-23 mb-3">
                                <ul>
                                    <li><span class="icon icon-map-marker"></span><span class="text">@item.Address</span></li>
                                    <li><span class="icon icon-phone"></span><span class="text">@item.Phone</span></li>
                                    <li><span class="icon icon-envelope"></span><span class="text">@item.Email</span></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 text-center">

                        <p>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            Copyright &copy;
                            <script>document.write(new Date().getFullYear());</script> Tüm hakları Saklıdır | This template is made with <i class="icon-heart color-danger" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Serhat KARATAŞLI</a>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        </p>
                    </div>
                </div>
            </div>
        </footer>
    }

    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>

