@model List<GetTagCloudByBlogIdDto>
<div class="sidebar-box ftco-animate">
    <h3>Etiket Bulutu</h3>
    <div class="tagcloud">
        @if (Model.Count > 0)
        {

            foreach (var item in Model)
            {
                <a href="#" class="tag-cloud-link">@item.Title</a>
            }

        }
    </div>
</div>